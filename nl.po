#: batticonplus.c:154
msgid "Display the version"
msgstr "Versie tonen"

#: batticonplus.c:155
msgid "Display debug information"
msgstr "Foutopsporingsinformatie tonen"

#: batticonplus.c:156
msgid "Set update interval (in seconds)"
msgstr "Update-interval definiëren (in seconden)"

#: batticonplus.c:157
msgid "Set icon type ('standard', 'notification' or 'symbolic')"
msgstr "Symbooltype definiëren ('standard', 'notification' of 'symbolic')"

#: batticonplus.c:158
msgid "Set low battery level (in percent)"
msgstr "Laag accu-niveau definiëren (in procenten)"

#: batticonplus.c:159
msgid "Set critical battery level (in percent)"
msgstr "Kritisch accu-niveau definiëren (in procenten)"

#: batticonplus.c:160
msgid "Command to execute when critical battery level is reached"
msgstr "Commando om uit te voeren bij kritisch accu-niveau"

#: batticonplus.c:161
msgid "Command to execute when left clicking on tray icon"
msgstr "Commando om uit te voeren bij linker klik op systeemvak icoon"

#: batticonplus.c:163
msgid "Hide the notification popups"
msgstr "De notificatie-popups verbergen"

#: batticonplus.c:165
msgid "List available icon types"
msgstr "Lijst van beschikbare icoontypen tonen"

#: batticonplus.c:166
msgid "List available power supplies (battery and AC)"
msgstr "Lijst van beschikbare voedingen tonen (accu en netstroom)"

#: batticonplus.c:170
msgid "[BATTERY ID]"
msgstr "[ACCU ID]"

#: batticonplus.c:174
#, c-format
msgid "Cannot parse command line arguments: %s\n"
msgstr "Kan commandoregel argumenten niet ontleden: %s\n"

#: batticonplus.c:185
msgid ""
"batticonplus: a lightweight and fast battery icon that sits in your system "
"tray\n"
msgstr ""
"batticonplus: een lichtgewicht en snel accusymbool dat in het "
"systeemvak zit\n"

#: batticonplus.c:186
#, c-format
msgid "version %s\n"
msgstr "versie %s\n"

#: batticonplus.c:194
msgid "List of available power supplies:\n"
msgstr "Lijst van beschikbare voedingen:\n"

#: batticonplus.c:209
msgid "List of available icon types:\n"
msgstr "Lijst van beschikbare icoontypen:\n"

#: batticonplus.c:210 batticonplus.c:211 batticonplus.c:212
msgid "available"
msgstr "beschikbaar"

#: batticonplus.c:210 batticonplus.c:211 batticonplus.c:212 batticonplus.c:540
#: batticonplus.c:548 batticonplus.c:570
msgid "unavailable"
msgstr "niet beschikbaar"

#: batticonplus.c:226
#, c-format
msgid "Unknown icon type: %s\n"
msgstr "Onbekend symbooltype: %s\n"

#: batticonplus.c:238
msgid "No icon type found!\n"
msgstr "Geen symbooltype gevonden!\n"

#: batticonplus.c:245
#, c-format
msgid "Invalid update interval! It has been reset to default (%d seconds)\n"
msgstr ""
"Ongeldig update-interval! Is naar standaardwaarde teruggezet "
"(%d seconden)\n"

#: batticonplus.c:252
#, c-format
msgid "Invalid low level! It has been reset to default (%d percent)\n"
msgstr ""
"Ongeldig laag niveau! Is naar standaardwaarde teruggezet "
"(%d procent)\n"

#: batticonplus.c:257
#, c-format
msgid "Invalid critical level! It has been reset to default (%d percent)\n"
msgstr ""
"Ongeldig kritisch niveau! Is naar standaardwaarde teruggezet "
"(%d procent)\n"

#: batticonplus.c:263
msgid ""
"Critical level is higher than low level! They have been reset to default\n"
msgstr ""
"Kritisch niveau is hoger dan laag niveau. Beide worden naar "
"de standaardwaarde teruggezet\n"

#: batticonplus.c:294 batticonplus.c:326
#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "type: %-*.*s\tID: %-*.*s\tpad: %s\n"

#: batticonplus.c:294
msgid "Battery"
msgstr "Accu"

#: batticonplus.c:326
msgid "AC"
msgstr "Netstroom"

#: batticonplus.c:348
#, c-format
msgid "Cannot open sysfs directory: %s (%s)\n"
msgstr "Kan sysfs map niet openen: %s (%s)\n"

#: batticonplus.c:355
#, c-format
msgid "No battery with suffix %s found!\n"
msgstr "Geen accu met achtervoegsel %s gevonden!\n"

#: batticonplus.c:360
msgid "No battery nor AC power supply found!\n"
msgstr "Geen accu noch netstroom gevonden!\n"

#: batticonplus.c:672 batticonplus.c:675
msgid "AC only, no battery!"
msgstr "Alleen netstroom, geen accu!"

#: batticonplus.c:793
#, c-format
msgid "Spawning critical battery level command in 30 seconds: %s"
msgstr ""
"Kritisch accuniveau commando wordt in 30 seconden opgestart: %s"

#: batticonplus.c:797
#, c-format
msgid "Cannot spawn critical battery level command: %s\n"
msgstr "Kan commando voor kritisch accuniveau niet opstarten: %s\n"

#: batticonplus.c:800
msgid "Cannot spawn critical battery level command!"
msgstr "Kan commando voor kritisch accuniveau niet opstarten!"

#: batticonplus.c:817
#, c-format
msgid "Cannot spawn left click command: %s\n"
msgstr "Kan commando voor linker klik niet opstarten: %s\n"

#: batticonplus.c:820
msgid "Cannot spawn left click command!"
msgstr "Kan commando voor linker klik niet opstarten!"

#: batticonplus.c:884
msgid "Battery is missing!"
msgstr "Accu ontbreekt!"

#: batticonplus.c:888
msgid "Battery status is unknown!"
msgstr "Accuniveau onbekend!"

#: batticonplus.c:892
msgid "Battery is charged!"
msgstr "Accu is opgeladen!"

#: batticonplus.c:896
msgid "Battery is discharging (%i%% remaining)"
msgstr "Accu ontlaadt (%i%% resterend)"

#: batticonplus.c:900
msgid "Battery is not charging (%i%% remaining)"
msgstr "Accu laadt niet op (%i%% resterend)"

#: batticonplus.c:904
msgid "Battery level is low! (%i%% remaining)"
msgstr "Accuniveau laag! (%i%% resterend)"

#: batticonplus.c:908
msgid "Battery level is critical! (%i%% remaining)"
msgstr "Accuniveau kritisch! (%i%% resterend)"

#: batticonplus.c:912
msgid "Battery is charging (%i%%)"
msgstr "Accu laadt op (%i%%)"

#: batticonplus.c:941
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d minuut"
msgstr[1] "%d minuten"

#: batticonplus.c:942
msgid "%d hour, %s remaining"
msgid_plural "%d hours, %s remaining"
msgstr[0] "%d uur, %s resterend"
msgstr[1] "%d uren, %s resterend"

#: batticonplus.c:944
msgid "%d minute remaining"
msgid_plural "%d minutes remaining"
msgstr[0] "%d minuut resterend"
msgstr[1] "%d minuten resterend"
