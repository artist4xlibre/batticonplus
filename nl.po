#: batticonplus.c:154
msgid "Display the version"
msgstr "Versie tonen"

#: batticonplus.c:155
msgid "Display debug information"
msgstr "Debug-informatie tonen"

#: batticonplus.c:156
msgid "Set update interval (in seconds)"
msgstr "Update-intervall definieren (in seconden)"

#: batticonplus.c:157
msgid "Set icon type ('standard', 'notification' or 'symbolic')"
msgstr "Symbooltype definieren ('standard', 'notification' of 'symbolic')"

#: batticonplus.c:158
msgid "Set low battery level (in percent)"
msgstr "Laag accu-niveau definieren (in procenten)"

#: batticonplus.c:159
msgid "Set critical battery level (in percent)"
msgstr "Kritisch accu-niveau definieren (in procenten)"

#: batticonplus.c:160
msgid "Command to execute when critical battery level is reached"
msgstr "Commando om uit te voeren bij kritisch accu-niveau"

#: batticonplus.c:161
msgid "Command to execute when left clicking on tray icon"
msgstr "Commando om uit te voeren bij linker klik op het tray-icoon"

#: batticonplus.c:163
msgid "Hide the notification popups"
msgstr "De notificatie-popups verbergen"

#: batticonplus.c:165
msgid "List available icon types"
msgstr "Lijst van beschikbare icoontypen tonen"

#: batticonplus.c:166
msgid "List available power supplies (battery and AC)"
msgstr "Lijst van beschikbare stroomvoedingen tonen (accu en netstroom)"

#: batticonplus.c:170
msgid "[BATTERY ID]"
msgstr "[ACCU ID]"

#: batticonplus.c:174
#, c-format
msgid "Cannot parse command line arguments: %s\n"
msgstr "Kan commandoregel argumenten niet parsen: %s\n"

#: batticonplus.c:185
msgid ""
"batticonplus: a lightweight and fast battery icon that sits in your system "
"tray\n"
msgstr ""
"batticonplus: een lichtgewicht en snelle accusymbol, dat in de "
"systeem-tray zit\n"

#: batticonplus.c:186
#, c-format
msgid "version %s\n"
msgstr "Versie %s\n"

#: batticonplus.c:194
msgid "List of available power supplies:\n"
msgstr "Lijst van beschikbare stroomvoedingen:\n"

#: batticonplus.c:209
msgid "List of available icon types:\n"
msgstr "Lijst van beschikbare icoontypen:\n"

#: batticonplus.c:210 batticonplus.c:211 batticonplus.c:212
msgid "available"
msgstr "beschikbaar"

#: batticonplus.c:210 batticonplus.c:211 batticonplus.c:212 batticonplus.c:540
#: batticonplus.c:548 batticonplus.c:570
msgid "unavailable"
msgstr "niet beschikbaar"

#: batticonplus.c:226
#, c-format
msgid "Unknown icon type: %s\n"
msgstr "Unbekannter Symboltyp: %s\n"

#: batticonplus.c:238
msgid "No icon type found!\n"
msgstr "Kein Symboltyp gefunden!\n"

#: batticonplus.c:245
#, c-format
msgid "Invalid update interval! It has been reset to default (%d seconds)\n"
msgstr ""
"Ungültiges Update-Intervall! Wird op Voreinstellung zurückgesetzt (%d "
"Sekunden)\n"

#: batticonplus.c:252
#, c-format
msgid "Invalid low level! It has been reset to default (%d percent)\n"
msgstr ""
"Ungültige Einstellung für niedrigen accustand! Wird op Voreinstellung "
"zurückgesetzt (%d Prozent)\n"

#: batticonplus.c:257
#, c-format
msgid "Invalid critical level! It has been reset to default (%d percent)\n"
msgstr ""
"Ungültige Einstellung für kritischen accustand! Wird op Voreinstellung "
"zurückgesetzt (%d Prozent)\n"

#: batticonplus.c:263
msgid ""
"Critical level is higher than low level! They have been reset to default\n"
msgstr ""
"Einstellung für kritischen accustand ist höher als für niedrigen. Beide "
"werden op die Voreinstellungen zurückgesetzt\n"

#: batticonplus.c:294 batticonplus.c:326
#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "Typ: %-*.*s\tID: %-*.*s\tPfad: %s\n"

