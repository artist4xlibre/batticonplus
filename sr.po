#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "тип: %-*.*s\tid: %-*.*s\tpath: %s\n"

#: batticonplus.c:294
msgid "Battery"
msgstr "Батерија"

#: batticonplus.c:326
msgid "AC"
msgstr "Мрежно напајање"

